<h1>Members</h1>

<a href="/members/new">New Member</a>

<table>
  <tr>
    <th>Name</th>
    <th>Membership</th>
    <th>Delete</th>
  </tr>
  <% for member in @members %>
  <tr>
    <td><a href="/members/<%= member.id %>"><%= member.name %></a></td>
    <td><%= member.premium %></td>
    <td>
      <form method="post" action="/members/<%= member.id %>/delete">
        <input type="submit" value="Delete"/>
      </form>
    </td>
  </tr>
  <% end %>
</table>